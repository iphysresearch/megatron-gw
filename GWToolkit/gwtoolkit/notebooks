      "from torch.utils.data import IterableDataset\n",
      "\n",
      "\n",
      "class TorchIterableDataset(IterableDataset):\n",
      "    def __init__(self, generator_func):\n",
      "        self.generator_func = generator_func\n",
      "\n",
      "    def __iter__(self):\n",
      "        it = self.generator_func()\n",
      "        yield from it\n"
     ]
    }
   ],
