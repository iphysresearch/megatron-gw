    "class BatchInferModel:\n",
    "    def __init__(self):\n",
    "        # self.model = ImageNetModel()\n",
    "        pass\n",
    "    def __call__(self, batch):\n",
    "        # return self.model(batch)\n",
    "        return batch\n",
    "    \n",
    "# Apply GPU batch inference to the data.\n",
    "pipe = ds.repeat(5)#.random_shuffle_each_window()\n",
    "pipe = pipe.map_batches(BatchInferModel, compute=\"actors\", \n",
    "                        batch_size=256, num_gpus=1, batch_format = 'native')\n",
    "iterable_dataset = to_torch(pipe)\n",
    "for i in iterable_dataset:\n",
    "    break\n",
    "i.shape"
