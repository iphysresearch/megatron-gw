    "plt.figure(figsize=(8,5), dpi=80)\n",
    "b1 = plt.plot(*a1, \"*-\", label='no parallel',)\n",
    "b2 = plt.plot(*a2, \"*-\", label='parallel')\n",
    "plt.plot(*stand, \"k:\", label=r'$10\\times$no parallel')\n",
    "plt.yscale('log')\n",
    "plt.xscale('log')\n",
    "plt.ylabel('Seconds')\n",
    "plt.xlabel('Num of samples')\n",
    "plt.axhline(0.3, color='k', linestyle='--')\n",
    "plt.scatter(128, 0.3, color='k', marker='^')\n",
    "plt.plot(x, a2[1]/x * 128, \":\", color=b2[0].get_markeredgecolor(), label = 'parallel for 128 samples')\n",
    "plt.plot(x, a1[1]/x * 128, \":\", color=b1[0].get_markeredgecolor(), label = 'no parallel for 128 samples')\n",
    "plt.legend();\n",
    "print(f'''Use ~{x[np.argmin(a2[1] - a1[1]/10)]} Actors for parallel is the closest to 10 x no parallel.\n",
    "Use ~{x[np.argmin(a2[1]/x * 128)]} Actors for parallel have the most efficient performance for 128 samples.''')"
