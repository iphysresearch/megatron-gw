   "source": [
    "ray.shutdown()\n",
    "ray.init(num_cpus = 5)\n",
    "\n",
    "num = 10\n",
    "\n",
    "@ray.remote\n",
    "def func(i):\n",
    "    index = [0.1, 0.9, 1.3, 1.4, 1.5]*(num//2)\n",
    "    time.sleep(index[i])\n",
    "    return np.ones(shape=(1, 2, 1024), dtype=np.float64)\n",
    "\n",
    "\n",
    "start = time.time()\n",
    "block = np.zeros(shape=(num, 2, 1024), dtype=np.float64)\n",
    "result_ids = [func.remote(i) for i in tqdm(range(num))]\n",
    "\n",
    "\n",
    "result = np.empty(shape=(0, 2, 1024), dtype=np.float64)\n",
    "while len(result_ids):\n",
    "    done_id, result_ids = ray.wait(result_ids)\n",
    "    result = np.concatenate([result, ray.get(done_id[0])])\n",
    "\n",
    "print(time.time() - start)"
   ]
