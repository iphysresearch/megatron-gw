   "source": [
    "def gen_(num):\n",
    "    result_ids = [func.remote(i) for i in range(num)]\n",
    "\n",
    "    result = np.empty(shape=(0, 2, 131072), dtype=np.float64)\n",
    "    while len(result_ids):\n",
    "        done_id, result_ids = ray.wait(result_ids)\n",
    "        result = np.concatenate([result, ray.get(done_id[0])])\n",
    "    return result"
   ]
