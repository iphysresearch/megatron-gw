     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "plt.plot(*cut_from_long(wfd.time_array, data))\n",
    "plt.plot(*cut_from_long(wfd.time_array, signal))\n",
    "plt.axvline(start_time + (duration_long-duration)/2, color = 'b')\n",
    "plt.axvline(start_time + (duration_long-duration)/2 + duration, color= 'b')\n",
    "plt.axvline(geocent_time[0], color='k', linestyle=':')\n",
    "plt.axvline(geocent_time[1], color='k', linestyle=':')\n",
    "plt.axvline(target_time, color='r')"
