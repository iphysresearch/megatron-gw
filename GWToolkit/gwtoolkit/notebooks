   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "aeceff21-5ee8-4b21-980d-f140d36ab67e",
   "metadata": {},
   "outputs": [],
   "source": [
    "from numpy.fft import rfft, irfft, rfftfreq\n",
    "from scipy.interpolate import interp1d\n",
    "from gwpy.timeseries import TimeSeries\n",
    "from gwpy.signal import filter_design\n",
    "\n",
    "\n",
    "def whiten(signal, sampling_rate, f_ASD, ASD, return_tilde=False):\n",
    "\n",
    "    f_signal = rfftfreq(len(signal), 1./sampling_rate)\n",
    "    psd = np.abs(interp1d(f_ASD, ASD**2, bounds_error=False, fill_value=np.inf)(f_signal))\n",
    "    signal_filtered_tilde = rfft(signal) / np.sqrt(0.5 * sampling_rate * psd)\n",
    "    if return_tilde:\n",
    "        return irfft(signal_filtered_tilde), signal_filtered_tilde\n",
    "    else:\n",
    "        return irfft(signal_filtered_tilde)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "77e8d435-6837-4500-ad39-0905ed6cc8da",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 2400x960 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "from gwpy.timeseries import TimeSeries\n",
    "addr_asds = [\n",
    "    # https://dcc.ligo.org/T1800044-v5/public\n",
    "    '/workspace/zhaoty/GWToolkit/gwtoolkit/gw/prior_files/aLIGODesign.txt', \n",
    "    # https://dcc.ligo.org/LIGO-T2000012/public\n",
    "    '/opt/conda/lib/python3.8/site-packages/bilby/gw/detector/noise_curves/aLIGO_ZERO_DET_high_P_asd.txt',\n",
    "    '/opt/conda/lib/python3.8/site-packages/bilby/gw/detector/noise_curves/aLIGO_early_asd.txt',\n",
    "    '/opt/conda/lib/python3.8/site-packages/bilby/gw/detector/noise_curves/aLIGO_early_high_asd.txt',\n",
    "    '/opt/conda/lib/python3.8/site-packages/bilby/gw/detector/noise_curves/aLIGO_mid_asd.txt',\n",
    "    '/opt/conda/lib/python3.8/site-packages/bilby/gw/detector/noise_curves/aLIGO_late_asd.txt',\n",
    "]\n",
    "bp = filter_design.bandpass(50, 250, hdata.sample_rate)\n",
    "notches = [filter_design.notch(line, hdata.sample_rate) for\n",
    "           line in (60, 120, 180)]\n",
    "zpk = filter_design.concatenate_zpks(bp, *notches)\n",
    "\n",
    "plt.figure(figsize=(20,8), dpi=120)\n",
    "for addr_asd in addr_asds:\n",
    "    data = np.loadtxt(addr_asd)\n",
    "    ASDf, ASD = data[:,0], data[:,1]\n",
    "    # plt.loglog(ASDf, ASD**2, label = addr_asd.split('/')[-1].split('.')[0])\n",
    "    whitened_strain = whiten(strain, 4096, ASDf, ASD, return_tilde=False)\n",
    "\n",
    "    # plt.figure(figsize=(10,5))\n",
    "    # plt.plot(time, whitened_strain, label = addr_asd.split('/')[-1].split('.')[0])\n",
    "\n",
    "    hdata = TimeSeries(whitened_strain, times=time, channel='H1')\n",
    "    hfilt = hdata.filter(zpk, filtfilt=True)\n",
    "    plt.plot(hfilt.times.value, hfilt.value,  label = addr_asd.split('/')[-1].split('.')[0])\n",
    "    # plt.axvline(target_time, color='r')\n",
    "    # plt.xlim(target_time-0.2, target_time+0.1);\n",
    "    # plt.legend();\n",
    "\n",
    "hdata = TimeSeries(strain, times=time, channel='H1')\n",
    "whiten_ = hdata.whiten(4, 2)\n",
    "# plt.figure(figsize=(10,5))\n",
    "# plt.plot(whiten_.times.value, whiten_.value, label = 'current on-source PSDs')\n",
    "plt.axvline(target_time, color='r', linestyle=':')\n",
    "plt.xlim(target_time-0.2, target_time+0.1);\n",
    "\n",
    "hdata = TimeSeries(whiten_.value, times=whiten_.times.value, channel='H1')\n",
    "hfilt = hdata.filter(zpk, filtfilt=True)\n",
    "plt.plot(hfilt.times.value, hfilt.value, label = 'around GW150914')\n",
    "plt.legend()\n",
    "\n",
    "# GW150914_template_H1 = np.load('/workspace/zhaoty/GWToolkit/gwtoolkit/gw/prior_files/GW150914_template_H1.npy')\n",
    "# GW150914_template_H1_time = np.load('/workspace/zhaoty/GWToolkit/gwtoolkit/gw/prior_files/GW150914_template_time.npy')\n",
    "# plt.plot(GW150914_template_H1_time, GW150914_template_H1 * 0.3, color = 'black')\n",
    "plt.ylim(-4,4);\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "e05ac1d0-ef20-42de-a088-ba2852f461e5",
   "metadata": {},
   "source": [
    "## Whitening"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "3623cf34-2bea-456d-ae00-289fef18f3ea",
   "metadata": {},
   "outputs": [],
   "source": [
    "from numpy.fft import rfft, irfft, rfftfreq\n",
    "from scipy.interpolate import interp1d\n",
    "from scipy.signal import welch\n",
    "\n",
    "def whiten(signal, sampling_rate, f_ASD=None, ASD=None, return_tilde=False, return_psd=False):\n",
    "    \"\"\"https://github.com/moble/MatchedFiltering/blob/3d7f3b5ca1383e26b80e64c82a703fe054f16c40/utilities.py#L155\n",
    "    \"\"\"\n",
    "    f_signal = rfftfreq(len(signal), 1./sampling_rate)\n",
    "    if ASD is not None:\n",
    "        psd = np.abs(interp1d(f_ASD, ASD**2, bounds_error=False, fill_value=np.inf)(f_signal))\n",
    "    else:\n",
    "        f_psd, psd = welch(signal, sampling_rate, nperseg=2**int(np.log2(len(signal)/8.0)), scaling='density')\n",
    "        psd = np.abs(interp1d(f_psd, psd, bounds_error=False, fill_value=np.inf)(f_signal))\n",
    "    signal_filtered_tilde = rfft(signal) / np.sqrt(0.5 * sampling_rate * psd)\n",
    "    if return_tilde:\n",
    "        return irfft(signal_filtered_tilde), signal_filtered_tilde\n",
    "    elif return_psd:\n",
    "        return irfft(signal_filtered_tilde), (f_signal, psd)\n",
    "    else:\n",
    "        return irfft(signal_filtered_tilde)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "ad01a06b-5551-4339-817b-39b5fff06ece",
   "metadata": {},
   "source": [
