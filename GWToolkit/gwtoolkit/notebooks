  {
   "cell_type": "code",
   "execution_count": null,
   "id": "fb1159e5-129d-4144-8615-b79ea6ea2da1",
   "metadata": {},
   "outputs": [],
   "source": [
    "def time_waveform_response_block(self, num, start_time, geocent_time=None, target_optimal_snr_tuple=None,\n",
    "                                 disable=True):\n",
    "    \"\"\"Generate a data block with detector responses of GW waveform in time domain\n",
    "    num: int\n",
    "        Number of data\n",
    "    geocent_time: tuple, optional, (minimum, maximum)\n",
    "        The GPS arrival time of the signal data\n",
    "    start_time: float (default: 0)\n",
    "        The GPS start-time of the data / Starting time of the time array\n",
    "    target_optimal_snr_tuple： tuple, optional, (target_detector_index, target_optimal_snr)\n",
    "\n",
    "    #TODO too-many-locals\n",
    "    \"\"\"\n",
    "    Record = namedtuple('Record', 'block noise optimal_snr matched_filter_snr meta_data block_whitened signal_whitened noise_whitened')\n",
    "    block = Record(numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64),\n",
    "                   numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64),\n",
    "                   numpy.empty(shape=(0, len(self.dets.keys())), dtype=numpy.float64),\n",
    "                   numpy.empty(shape=(0, len(self.dets.keys())), dtype=numpy.complex64),\n",
    "                   {},\n",
    "                   numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64),\n",
    "                   numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64),\n",
    "                   numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64))\n",
    "    # meta_data = {}\n",
    "    alpha = 1\n",
    "    # noise_freq_block = self.frequency_colored_noise_block(num)\n",
    "    # noise_block = numpy.empty(shape=(num, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    # optimal_snr = numpy.empty(shape=(num, len(self.dets.keys())), dtype=numpy.float64)\n",
    "    # matched_filter_snr = numpy.empty(shape=(num, len(self.dets.keys())), dtype=numpy.complex64)\n",
    "    # block = numpy.empty(shape=(num, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    # snr_flat = numpy.random.rand(num) * 45 + 5\n",
    "    @ray.remote\n",
    "    def _inside_func():\n",
    "        self._update_waveform()\n",
    "        self.update_detector_response(start_time=start_time, geocent_time=geocent_time)\n",
    "\n",
    "        if isinstance(target_optimal_snr_tuple, Iterable) and target_optimal_snr_tuple[1]:\n",
    "\n",
    "            optimal_snr_cache = self.dets[list(self.dets.keys())[target_optimal_snr_tuple[0]]].optimal_snr(\n",
    "                self.frequency_waveform_response[target_optimal_snr_tuple[0]]\n",
    "            )\n",
    "            alpha = target_optimal_snr_tuple[1] / optimal_snr_cache\n",
    "            optimal_snr = numpy.concatenate(\n",
    "                [numpy.asarray([target_optimal_snr_tuple[1]])\n",
    "                 if j == target_optimal_snr_tuple[0] else\n",
    "                 numpy.asarray([det.optimal_snr(self.frequency_waveform_response[j] * alpha)])\n",
    "                 for j, (_, det) in enumerate(self.dets.items())\n",
    "                 ],\n",
    "            )\n",
    "            # block.optimal_snr[i] = snr_flat[i]\n",
    "            # alpha = snr_flat[i] / optimal_snr_cache\n",
    "        else:\n",
    "            optimal_snr = numpy.concatenate(\n",
    "                [numpy.asarray([det.optimal_snr(\n",
    "                 self.frequency_waveform_response[j] * alpha)])\n",
    "                 for j, (_, det) in enumerate(self.dets.items())\n",
    "                 ],\n",
    "            )\n",
    "\n",
    "        block = self.time_waveform_response * alpha\n",
    "        block_whitened = numpy.concatenate(\n",
    "            [numpy.asarray([det.frequency_to_time_domain(det.whiten(\n",
    "             self.frequency_waveform_response[j] * alpha + self.frequency_colored_noise[j]))[0]])\n",
    "             for j, (_, det) in enumerate(self.dets.items())\n",
    "             ],\n",
    "        )\n",
    "\n",
    "        signal_whitened = numpy.concatenate(\n",
    "            [numpy.asarray([det.frequency_to_time_domain(det.whiten(\n",
    "             self.frequency_waveform_response[j] * alpha ))[0]])\n",
    "             for j, (_, det) in enumerate(self.dets.items())\n",
    "             ],\n",
    "        )\n",
    "\n",
    "        noise_whitened = numpy.concatenate(\n",
    "            [numpy.asarray([det.frequency_to_time_domain(det.whiten(\n",
    "             self.frequency_colored_noise[j]))[0]])\n",
    "             for j, (_, det) in enumerate(self.dets.items())\n",
    "             ],\n",
    "        )\n",
    "\n",
    "        matched_filter_snr = numpy.concatenate(\n",
    "            [numpy.asarray([det.matched_filter_snr(\n",
    "             self.frequency_waveform_response[j] * alpha,\n",
    "             self.frequency_waveform_response[j] * alpha + self.frequency_colored_noise[j])])\n",
    "             for j, (_, det) in enumerate(self.dets.items())\n",
    "             ],\n",
    "        )\n",
    "        \n",
    "        # meta_data = {}\n",
    "        # for key, value in self.parameters.items():\n",
    "        #     if key not in meta_data:\n",
    "        #         meta_data[key] = numpy.empty([num, ])\n",
    "        #     meta_data[key][i] = value\n",
    "\n",
    "        noise = numpy.concatenate(\n",
    "            [det.frequency_to_time_domain(self.frequency_colored_noise[j])[0][numpy.newaxis,...]\n",
    "            for j, (_, det) in enumerate(self.dets.items())\n",
    "            ],\n",
    "        )\n",
    "\n",
    "        return (block[numpy.newaxis,...], noise[numpy.newaxis,...],\n",
    "                block_whitened[numpy.newaxis,...], \n",
    "                signal_whitened[numpy.newaxis,...], noise_whitened[numpy.newaxis,...], \n",
    "                optimal_snr, matched_filter_snr, \n",
    "                self.parameters)\n",
    "\n",
    "    # Way 1\n",
    "    data = ray.get([_inside_func.remote() for _ in tqdm(range(num))])\n",
    "    data = numpy.asarray(data, dtype=object)\n",
    "    meta_data = {key: numpy.concatenate([data[i,-1][key] for i in range(num)]) for key in data[0,-1].keys()}\n",
    "    meta_data['optimal_snr'] = numpy.concatenate(data[:,5])\n",
    "    meta_data['matched_filter_snr'] = numpy.concatenate(data[:,6])\n",
    "    return (numpy.concatenate(data[:,0]),\n",
    "        meta_data,\n",
    "        numpy.concatenate(data[:,1]),\n",
    "        numpy.concatenate(data[:,2]),\n",
    "        numpy.concatenate(data[:,3]),\n",
    "        numpy.concatenate(data[:,4]))\n",
    "\n",
    "    # Way 2\n",
    "    def process_incremental(data, new_data):\n",
    "        # new_data = numpy.asarray(new_data, dtype=object)\n",
    "        data[0] = numpy.concatenate([data[0], new_data[0]])\n",
    "        data[1] = numpy.concatenate([data[1], new_data[1]])\n",
    "        data[2] = numpy.concatenate([data[2], new_data[2]])\n",
    "        data[3] = numpy.concatenate([data[3], new_data[3]])\n",
    "        data[4] = numpy.concatenate([data[4], new_data[4]])\n",
    "        return data\n",
    "\n",
    "    data_ids = [_inside_func.remote() for _ in tqdm(range(num))]\n",
    "    \n",
    "    # data = [block.block, block.noise, block.block_whitened, block.signal_whitened, block.noise_whitened, block.meta_data]\n",
    "    block = numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    noise = numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    block_whitened = numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    signal_whitened = numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    noise_whitened = numpy.empty(shape=(0, len(self.dets.keys()), self.num_t), dtype=numpy.float64)\n",
    "    while len(data_ids):\n",
    "        done_id, data_ids = ray.wait(data_ids)\n",
    "        # data = process_incremental(data, ray.get(done_id[0]))\n",
    "        # data = ray.get(done_id[0])\n",
    "        block = numpy.concatenate([block, ray.get(done_id[0])[0]])\n",
    "        noise = numpy.concatenate([noise, ray.get(done_id[0])[1]])\n",
    "        block_whitened = numpy.concatenate([block_whitened, ray.get(done_id[0])[2]])\n",
    "        signal_whitened = numpy.concatenate([signal_whitened, ray.get(done_id[0])[3]])\n",
    "        noise_whitened = numpy.concatenate([noise_whitened, ray.get(done_id[0])[4]])\n",
    "    return block, noise, block_whitened, signal_whitened, noise_whitened\n",
    "\n",
    "#     block.block, block.meta_data, block.noise, block.block_whitened, block.signal_whitened, block.noise_whitened\n",
    "    \n",
    "#     meta_data = {key: numpy.concatenate([data[i,-1][key] for i in range(num)]) for key in data[0,-1].keys()}\n",
    "#     meta_data['optimal_snr'] = numpy.concatenate(data[:,5])\n",
    "#     meta_data['matched_filter_snr'] = numpy.concatenate(data[:,6])\n",
    "#     return (numpy.concatenate(data[:,0]),\n",
    "#     meta_data,\n",
    "#     numpy.concatenate(data[:,1]),\n",
    "#     numpy.concatenate(data[:,2]),\n",
    "#     numpy.concatenate(data[:,3]),\n",
    "#     numpy.concatenate(data[:,4]))\n"
   ]
  },
