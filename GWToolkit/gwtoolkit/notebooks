   "outputs": [],
   "source": [
    "ray.shutdown()\n",
    "ray.init(num_cpus = 5)\n",
    "\n",
    "num = 5\n",
    "\n",
    "@ray.remote\n",
    "def func():\n",
    "    time.sleep(1)\n",
    "    return np.ones(shape=(1, 2, 1024), dtype=np.float64)\n",
    "\n",
    "\n",
    "start = time.time()\n",
    "block = np.zeros(shape=(num, 2, 1024), dtype=np.float64)\n",
    "result = ray.get([func.remote() for i in tqdm(range(num))])\n",
    "\n",
    "result = np.concatenate(result)\n",
    "print(time.time() - start)"
   ]
