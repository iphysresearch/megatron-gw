    "import sys\n",
    "sys.path.append('..')\n",
    "from gwtoolkit.gw.ray import RayDatasetTorch, RayDataset\n",
    "\n",
    "import time\n",
    "from tqdm import tqdm\n",
    "from torch.utils.data import DataLoader"
   ]
