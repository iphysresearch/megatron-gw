    "from gwtoolkit.gw.readligo import FileList, getstrain_cvmfs\n",
    "data_dir = '/workspace/zhaoty/dataset/O1_H1_All/'\n",
    "start = 1126259446\n",
    "filelist = FileList(directory=data_dir)\n",
    "strain, time = getstrain_cvmfs(start, start + 32, 'H1', filelist, inj_dq_cache=0)\n",
    "\n",
    "from gwpy.timeseries import TimeSeries\n",
    "\n",
    "hdata = TimeSeries(strain, times=time, channel='H1')#TimeSeries.fetch_open_data('H1', 1126259446, 1126259478)\n",
    "sg = hdata.spectrogram2(fftlength=4, overlap=2, window='hanning') ** (1/2.)\n",
    "psd_median = sg.percentile(50)"
   ]
