    "## Cut for the middel"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "035f51dc-d334-4f21-8c56-193a9f95bd50",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cut_from_long(time, data):\n",
    "    left_index = int((duration_long - duration)/2*sampling_frequency)\n",
    "    right_index = int((duration_long + duration)/2*sampling_frequency)\n",
    "    return time[left_index: right_index], data[left_index: right_index]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "b501d6fd-34a7-4d3b-96e5-a829289da2a0",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "(131072,)"
      ]
     },
     "execution_count": 25,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "## designed PSDs\n",
    "data = np.loadtxt(addr_asds[2])\n",
    "ASDf, ASD = data[:,0], data[:,1]\n",
    "\n",
    "## current on-source PSDs\n",
    "ASDf, ASD = None, None\n",
    "\n",
    "noise, (fpsd, psd) = whiten(wfd.dets[det].ifo.strain_data.time_domain_strain, \n",
    "                    4096, ASDf, ASD, \n",
    "                    return_psd=True)\n",
    "signal, _ = whiten(wfd.time_waveform_response[0], # H1\n",
    "                4096, fpsd, np.sqrt(psd),\n",
    "                return_psd=True)\n",
    "data = signal + noise\n",
    "data.shape"
   ]
  },
